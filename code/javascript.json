{
  "Style JSX": {
    "prefix": "stylejsx",
    "body": [
      "<style jsx>{`\n\t$1\n`}</style>"
    ],
    "description": "Style a next.js component with styled-jsx"
  },
  "Style JSX Global": {
    "prefix": "stylejsx",
    "body": [
      "<style jsx global>{`\n\t$1\n`}</style>"
    ],
    "description": "Globally style a next.js component/app with styled-jsx"
  },
  "Styled Components Theme Props": {
    "prefix": "tp",
    "body": [
      "${props => props.theme.$1}"
    ],
    "description": "Theme Props"
  }
}
