'.source.jade':

    # script snippets

    'script':
        'prefix': 'script.source'
        'body': '''
            script(type='${1:text/javascript}', src='${2:js/main.js}')
            $3
        '''

    'script.':
        'prefix': 'script.inline'
        'body': '''
            script.
                $1
        '''
